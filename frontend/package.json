{
  "name": "NCIApp",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "proxy": "http://app.djnci.local:9000",
  "devDependencies": {
    "@babel/runtime": "7.0.0-beta.55",
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "prettier-eslint": "^8.8.2",
    "prettier-stylelint": "^0.4.2",
    "react-snap": "^1.15.0",
    "source-map-explorer": "^1.5.0"
  },
  "dependencies": {
    "apollo-cache-inmemory": "^1.2.5",
    "apollo-client": "^2.3.5",
    "apollo-client-preset": "^1.0.8",
    "apollo-link-http": "^1.5.4",
    "bootstrap": "^3.3.7",
    "custom-react-scripts": "^0.2.2",
    "font-awesome": "^4.7.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "js-cookie": "^2.2.0",
    "lodash": "^4.17.10",
    "moment": "^2.22.2",
    "node-localstorage": "^1.3.1",
    "prop-types": "^15.6.2",
    "raven-js": "^3.26.3",
    "react": "^16.4.1",
    "react-apollo": "^2.1.6",
    "react-bootstrap": "^0.32.1",
    "react-bootstrap-table": "^4.3.1",
    "react-bootstrap-typeahead": "^3.1.4",
    "react-day-picker": "^7.1.10",
    "react-dom": "^16.4.1",
    "react-filterable-table": "^0.3.426",
    "react-fontawesome": "^1.6.1",
    "react-ga": "^2.5.3",
    "react-helmet": "5.2.0",
    "react-jsonschema-form": "^1.0.3",
    "react-jsonschema-form-extras": "^0.9.24",
    "react-moment": "^0.7.7",
    "react-progress-button": "^5.1.0",
    "react-raven": "^1.2.3",
    "react-render-html": "^0.6.0",
    "react-router": "^3.2.1",
    "react-rte": "^0.16.1",
    "react-search-input": "^0.11.3",
    "react-stripe-elements": "^2.0.1",
    "superagent": "^3.8.3"
  },
  "scripts": {
    "analyze": "source-map-explorer build/static/js/main.*",
    "start": "react-scripts start",
    "build": "react-scripts build && react-snap",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "babel": {
    "presets": [
      "env",
      "stage-0",
      "react"
    ]
  },
  "eslintConfig": {
    "extends": "airbnb",
    "globals": {
      "localStorage": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 7,
      "ecmaFeatures": {
        "jsx": true,
        "modules": true,
        "arrowFunctions": true,
        "classes": true,
        "spread": true,
        "experimentalObjectRestSpread": true
      }
    },
    "rules": {
      "strict": 0,
      "no-use-before-define": 0,
      "import/prefer-default-export": 0,
      "keyword-spacing": 2,
      "comma-dangle": [
        "error",
        "never"
      ],
      "react/no-danger": 0,
      "react/jsx-filename-extension": [
        1,
        {
          "extensions": [
            ".js",
            ".jsx"
          ]
        }
      ]
    },
    "plugins": [
      "eslint-plugin-react",
      "eslint-plugin-import",
      "eslint-plugin-jsx-a11y"
    ],
    "env": {
      "browser": true,
      "es6": true,
      "node": true
    }
  }
}
